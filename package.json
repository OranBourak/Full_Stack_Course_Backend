{
  "name": "full_stack_course_backend",
  "version": "1.0.0",
  "description": "sce full stack course",
  "main": "app.js",
  "scripts": {
    "start": "shx cp .env_dev .env && tsc && node dist/src/App.js",
    "dev": "shx cp .env_dev .env && nodemon ./src/App.ts",
    "test": "shx cp .env_test .env && jest --detectOpenHandles --forceExit --coverage",
    "testStudent": "shx cp .env_test .env && jest --detectOpenHandles --forceExit --coverage ./src/tests/student.test.ts",
    "testAuth": "shx cp .env_test .env && jest --detectOpenHandles --forceExit --coverage ./src/tests/auth.test.ts",
    "testPost": "shx cp .env_test .env && jest --detectOpenHandles --forceExit --coverage ./src/tests/post.test.ts",
    "testItem": "shx cp .env_test .env && jest --detectOpenHandles --forceExit --coverage ./src/tests/item.test.ts",
    "testFile": "shx cp .env_test .env && jest --detectOpenHandles --forceExit --coverage ./src/tests/file.test.ts",
    "lint": "eslint . --ext .ts"
  },
  "author": "Oran Bourak",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "google-auth-library": "^9.9.0",
    "jsonwebtoken": "^9.0.2",
    "latest": "^0.2.0",
    "mongoose": "^8.1.1",
    "multer": "^1.4.5-lts.1",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/mz": "^2.7.8",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "shx": "^0.3.4",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
